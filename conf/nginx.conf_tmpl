upstream tajle {
    server unix:///home/apps/tajle/tmp/tajle.sock;
}

server {
    listen 80;
    server_name tajle.grimcamera.com;
    charset utf-8;
    root /home/apps/tajle/pages;
}


server {
    listen 80;
    server_name apps.chladnicka.com;
    charset utf-8;
    client_max_body_size 10M;

    location / {
        uwsgi_pass tajle;
        include uwsgi_params;
    }
}
